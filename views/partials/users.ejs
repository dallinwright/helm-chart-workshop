<div class="container">
    <table class="table table-striped">
        <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Created At</th>
            <th>Updated At</th>
        </tr>
        </thead>
        <tbody>
        <!-- User data will be inserted here -->
        <% users.forEach(user => { %>
            <tr>
                <td><%= user.id %></td>
                <td><%= user.name %></td>
                <td><%= user.createdAt %></td>
                <td><%= user.updatedAt %></td>
            </tr>
        <% }); %>
        </tbody>
    </table>
</div>
